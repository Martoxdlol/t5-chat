{
    "name": "t5-chat",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "npm-run-all --parallel dev:app dev:server",
        "build": "pnpm run build:app && pnpm run build:server",
        "dev:app": "vite",
        "dev:server": "tsx --watch --env-file=.env src/server/main.ts",
        "build:app": "tsc -b && vite build",
        "build:server": "esbuild src/server/main.ts --bundle --platform=node --outfile=dist/server/main.cjs --format=cjs",
        "preview:app": "vite preview",
        "lint": "biome check",
        "format": "biome format --write",
        "db:push": "drizzle-kit push",
        "auth:generate": " pnpm dlx @better-auth/cli generate --output src/server/db/auth-schema.ts"
    },
    "dependencies": {
        "@ai-sdk/openai": "^1.3.22",
        "@hono/node-server": "^1.14.4",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-slot": "^1.2.3",
        "@tailwindcss/vite": "^4.1.8",
        "@tanstack/react-query": "^5.80.6",
        "@tanstack/react-virtual": "^3.13.10",
        "@trpc/client": "^11.3.1",
        "@trpc/server": "^11.3.1",
        "@trpc/tanstack-react-query": "^11.3.1",
        "ai": "^4.3.16",
        "better-auth": "^1.2.8",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "drizzle-orm": "^0.44.2",
        "hono": "^4.7.11",
        "katex": "^0.16.22",
        "lucide-react": "^0.513.0",
        "mysql2": "^3.14.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-error-boundary": "^6.0.0",
        "react-markdown": "^10.1.0",
        "react-router": "^7.6.2",
        "react-syntax-highlighter": "^15.6.1",
        "rehype-katex": "^7.0.1",
        "remark-gfm": "^4.0.1",
        "remark-math": "^6.0.0",
        "superjson": "^2.2.2",
        "tailwind-merge": "^3.3.0",
        "tailwindcss": "^4.1.8",
        "uuid": "^11.1.0",
        "zod": "^3.25.56"
    },
    "devDependencies": {
        "@better-auth/cli": "^1.2.8",
        "@biomejs/biome": "2.0.0-beta.6",
        "@react-scan/vite-plugin-react-scan": "^0.1.8",
        "@types/node": "^22.15.30",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@vitejs/plugin-react": "^4.4.1",
        "dotenv": "^16.5.0",
        "drizzle-kit": "^0.31.1",
        "esbuild": "^0.25.5",
        "globals": "^16.0.0",
        "npm-run-all": "^4.1.5",
        "react-scan": "^0.3.4",
        "tsx": "^4.19.4",
        "tw-animate-css": "^1.3.4",
        "typescript": "~5.8.3",
        "vite": "^6.3.5"
    }
}
