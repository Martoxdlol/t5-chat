{
  "name": "t5-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:app dev:server",
    "dev:app": "vite",
    "dev:server": "tsx --watch --env-file=.env src/server/main.ts",
    "build:app": "tsc -b && vite build",
    "preview:app": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "hono": "^4.7.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}